<h1>Sign Up</h1>
<%= form_for @user do |f|%>
    <%= f.label :email%><br>
    <%= f.text_field :email%><br>
    <%= f.label :firstName%><br>
    <%= f.text_field :firstName%><br>
    <%= f.label :lastName%><br>
    <%= f.text_field :lastName%><br>
    <%= f.label :role%><br>
    <%= f.text_field :role%><br>
    <%= f.label :password%><br>
    <%= f.password_field :password%><br>
    <%= f.submit %>
<%end%>
